<h1>Edit Film</h1>

<h1><%= @year.year %></h1> YOU ARE HERE!

<h2><%= @film.title %></h2>


<%= form_for :film do |f| %>

  <%= f.text_field :title %>
  <%= f.text_field :director %>
  <%= f.text_field :release_date %>
  <%= f.text_field :genre %>
  <%= f.text_field :opening_gross %>
  <%= f.submit  %>
<% end %>
